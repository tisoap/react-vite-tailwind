		"prettier": "prettier --check .",
		"preview": "vite preview",
		"spellcheck": "cspell --no-progress './src/**/*'",
		"storybook": "storybook dev -p 6006",
		"test-storybook": "vitest --project=storybook",
		"typecheck": "tsc --noEmit --project ./tsconfig.app.json"
